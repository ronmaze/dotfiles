{
	"[python]": {
		"editor.formatOnType": true
	},
	"[markdown]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"better-comments.tags": [
		{
			"backgroundColor": "transparent",
			"color": "#FF2D00",
			"strikethrough": false,
			"tag": "!"
		},
		{
			"backgroundColor": "transparent",
			"color": "#3498DB",
			"strikethrough": false,
			"tag": "?"
		},
		{
			"backgroundColor": "transparent",
			"color": "#474747",
			"strikethrough": true,
			"tag": "//"
		},
		{
			"backgroundColor": "transparent",
			"color": "#FF8C00",
			"strikethrough": false,
			"tag": "todo"
		},
		{
			"backgroundColor": "transparent",
			"color": "#98C379",
			"strikethrough": false,
			"tag": "*"
		}
	],
	"diffEditor.ignoreTrimWhitespace": false,
	"editor.autoClosingBrackets": "never",
	"editor.autoClosingDelete": "never",
	"editor.autoClosingOvertype": "never",
	"editor.autoClosingQuotes": "never",
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"editor.fontFamily": "CaskaydiaCove Nerd Font",
	"editor.fontLigatures": true,
	"editor.fontSize": 18,
	"editor.formatOnPaste": true,
	"editor.formatOnSave": true,
	"editor.formatOnType": true,
	"editor.lineNumbers": "relative",
	"editor.tabSize": 2,
	"editor.wordWrap": "wordWrapColumn",
	"editor.wordWrapColumn": 120,
	"extensions.ignoreRecommendations": true,
	"files.autoSave": "onFocusChange",
	"files.hotExit": "off",
	"files.insertFinalNewline": false,
	"files.trimTrailingWhitespace": true,
	"interactiveSession.editor.wordWrap": "on",
	"prettier.printWidth": 92,
	"prettier.requireConfig": false,
	"prettier.semi": true,
	"prettier.singleQuote": true,
	"prettier.useTabs": true,
	"redhat.telemetry.enabled": false,
	"settingsSync.ignoredExtensions": [],
	"settingsSync.ignoredSettings": ["-jupyter.variableQueries"],
	"telemetry.telemetryLevel": "off",
	"terminal.external.osxExec": "terminal.app",
	"terminal.integrated.fontSize": 16,
	"terminal.integrated.fontWeight": "normal",
	"terminal.integrated.fontWeightBold": "normal",
	"window.newWindowDimensions": "maximized",
	"window.title": "${dirty}${activeEditorMedium}",
	"workbench.iconTheme": "eq-material-theme-icons-darker",
	"workbench.startupEditor": "none",
	"python.analysis.completeFunctionParens": true,
	"editor.cursorBlinking": "smooth",
	"editor.cursorSurroundingLinesStyle": "all",
	"editor.fontVariations": false,
	"diffEditor.codeLens": true,
	"editor.minimap.autohide": true,
	"diffEditor.wordWrap": "on",
	"files.exclude": {
		"**/.lock": true,
		"**/.icloud": true,
		".apdisk": true,
		".AppleDB": true,
		".AppleDesktop": true,
		".AppleDouble": true,
		".com.apple.timemachine.donotpresent": true,
		".DocumentRevisions-V100": true,
		".DS_Store": true,
		".fseventsd": true,
		".LSOverride": true,
		".Spotlight-V100": true,
		".TemporaryItems": true,
		".Trashes": true,
		".VolumeIcon.icns": true
	},
	"security.workspace.trust.untrustedFiles": "open",
	"git.openRepositoryInParentFolders": "always",
	"workbench.preferredDarkColorTheme": "Default Dark+ Experimental",
	"window.autoDetectColorScheme": true,
	"window.titleBarStyle": "custom",
	"window.zoomLevel": 0,
	"workbench.preferredHighContrastColorTheme": "Default Dark+",
	"workbench.fontAliasing": "auto",
	"workbench.tips.enabled": false,
	"workbench.settings.useSplitJSON": false,
	"explorer.decorations.badges": false,
	"editor.lineHeight": 28,
	"editor.minimap.maxColumn": 80,
	"editor.minimap.renderCharacters": false,
	"editor.padding.top": 16,
	"editor.renderWhitespace": "selection",
	"editor.rulers": [80, 100, 120],
	"prettier.trailingComma": "all",
	"markdown.extension.italic.indicator": "_",
	"markdown.extension.preview.autoShowPreviewToSide": false,
	"yaml.validate": false,
	"bashIde.explainshellEndpoint": "http://localhost:5000",
	"[csv][dynamic csv][tsv][csv (semicolon)]": {
		"editor.stickyScroll.enabled": true
	},
	"files.associations": {
		"*.dat": "csv (pipe)",
		"*.csv": "csv (semicolon)"
	},
	"indentRainbow.colors": [
		"rgba(255,255,64,0.07)",
		"rgba(127,255,127,0.07)",
		"rgba(255,127,255,0.07)",
		"rgba(79,236,236,0.07)"
	],
	"indentRainbow.errorColor": "rgba(128,32,32,0.6)",
	"indentRainbow.tabmixColor": "rgba(128,32,96,0.6)",
	"indentRainbow.ignoreLinePatterns": [
		"/[ \t]* [*]/g", // lines begining with <whitespace><space>*
		"/[ \t]+[/]{2}/g" // lines begininning with <whitespace>//
	],
	"indentRainbow.ignoreErrorLanguages": ["markdown", "haskell"],
	"indentRainbow.colorOnWhiteSpaceOnly": true,
	"workbench.editor.splitInGroupLayout": "vertical", // false is the default
	"workbench.colorCustomizations": {
		"statusBar.background": "#1b1b1b",
		"statusBar.foreground": "#fff",
		"statusBar.noFolderBackground": "#3d3d3d",
		"statusBar.noFolderForeground": "#fff"
	},
	"workbench.colorTheme": "Default Dark+ Experimental",
	"git.confirmSync": false,
	"workbench.activityBar.visible": true,
	"window.commandCenter": false
}
